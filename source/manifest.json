{
  "manifest_version": 2,

  "name": "Magnet Links Parser for btkitty",
  "description": "This extension can parser the magnet links in search result page.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://cnbtkitty.xyz/; object-src 'self'"
}
